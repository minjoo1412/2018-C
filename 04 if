#include <stdio.h>

int main() {
	int n;
	scanf_s("%d", &n);

	if (n % 2 == 0) {
		printf("n은 짝수\n");
	}
	else {
		printf("n은 홀수\n");
	}

	int a, b, c;
	int max;
	scanf_s("%d%d%d", &a, &b, &c);

	if (a > b) {
		max = a;
		if (max > c) {
			printf("%d", max);
		}
		else {
			max = c;
			printf("%d", max);
		}
	}
	else {
		max = b;
		if (max > c) {
			printf("%d", max);
		}
		else {
			max = c;
			printf("%d", max);
		}
	}


}
